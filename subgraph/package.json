{
  "name": "aurora-subgraph-example",
  "version": "0.1.0",
  "repository": "https://github.com/aurora-is-near/aurora-examples/subgraph",
  "license": "MIT",
  "scripts": {
    "compile": "npx hardhat compile",
    "subgraph:create": "graph create --node http://localhost:8020 aurora/CovidVaccineToken",
    "subgraph:codegen": "npx graph codegen --output-dir src/types/",
    "subgraph:build": "graph build",
    "subgraph:deploy": "graph deploy example --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "clean": "rm -rf artifacts/ cache/ abis/* bin/*",
    "deploy:aurora": "yarn compile && npx hardhat run scripts/deploy.js --network aurora"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.1",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@openzeppelin/contracts": "^4.2.0",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.3.0",
    "hardhat": "^2.3.0",
    "hardhat-abi-exporter": "^2.2.1",
    "web3": "^1.3.6"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "dotenv": "^9.0.2"
  },
  "resolutions": {
    "assemblyscript": "git+https://github.com/AssemblyScript/assemblyscript.git#v0.6"
  }
}
